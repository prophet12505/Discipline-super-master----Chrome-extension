{
  "manifest_version": 2,
  "name": "Discipline Super Master",
  "description": "Get more done with BlockSite--the flexible Chrome extension! Block distracting websites by URL, keyword, set wake-up times, export/import settings. Stay focused.",
  "version": "1.0",
  "background": {
    "scripts": ["backgroundTest.js"]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/*",
    "browserAction",
    "options_ui",
    "contextMenus",
    "storage"
  ],
  "options_page": "option-panel.html",
  "browser_action": {
    "default_popup": "drop-down-panel.html"
  },
  "icons": {
    "128": "favicon.png"
  },
  "content_scripts":[{
    "matches":["<all_urls>"],
    "js":["content-script.js"]
    
  }],
  "content_security_policy": "script-src 'self' 'unsafe-inline'"
}
